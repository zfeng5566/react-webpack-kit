{
    "parser": "babel-eslint",
    "env": {"browser": true},
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
      "no-console":"warn",
      "react/jsx-uses-react": 1
    }
}